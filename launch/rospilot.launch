<launch>
  <node name="mavlink"
        pkg="rospilot"
        type="mavlink.py"
        respawn="false"
        output="screen"
        args="--baudrate=115200 --device=/dev/ttyACM0 --udp-export=127.0.0.1:8866"/>
  <node name="web_ui"
        pkg="rospilot"
        type="web_ui.py"
        respawn="false"
        output="screen"/>
  <node name="socket_io_server"
        pkg="rospilot"
        type="socket_io_server.py"
        respawn="false"
        output="screen"
        args="--server_port=8080 --policy_port=10843"/>
</launch>
